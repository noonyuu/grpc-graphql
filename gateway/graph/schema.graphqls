# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  name: String!
  age: Int!
}

type Query {
  users: [User!]!
}

input NewUser {
  name: String!
  age: Int!
}

input UserAttributes {
  id: ID!
  name: String!
  age: Int!
}

type Mutation {
  createUser(input: NewUser!): User!
  updateUser(input: UserAttributes!): User!
}